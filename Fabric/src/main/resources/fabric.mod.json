{
  "schemaVersion": 1,
  "id": "chunkbychunk",
  "version": "${version}",

  "name": "Chunk By Chunk",
  "description": "This mod starts the overworld with a single chunk, but provides the tools to expand to world one chunk at a time.",
  "authors": [
    "Immortius"
  ],
  "contact": {
    "homepage": "https://immortius.xyz/chunkbychunk",
    "sources": "https://github.com/immortius/chunkbychunk"
  },

  "license": "MIT License",
  "icon": "assets/chunkbychunk/textures/block/chunkspawner-north.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "xyz.immortius.chunkbychunk.fabric.ChunkByChunkMod"
    ],
    "client": [
      "xyz.immortius.chunkbychunk.fabric.ChunkByChunkClientMod"
    ],
    "modmenu": [ "xyz.immortius.chunkbychunk.fabric.ModMenuApiImpl" ],
    "jei_mod_plugin": [
      "xyz.immortius.chunkbychunk.common.jei.CBCJeiPlugin"
    ]
  },
  "mixins": [
    "chunkbychunk.mixins.json"
  ],
  "accessWidener" : "chunkbychunk.aw",
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": ">=1.20.1",
    "java": ">=17"
  },
  "suggests" : {
    "modmenu" : ">=7.1.0",
    "jei" : ">=15.1.0.19"
  }
}
  